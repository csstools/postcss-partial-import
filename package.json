    "postcss-tape": "2.0.1",
    "pre-commit": "^1.2.2"
  },
  "eslintConfig": {
    "extends": "dev"
  },
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "sugar",
    "sass",
    "scss",
